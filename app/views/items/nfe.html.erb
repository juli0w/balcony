<div class="container">
  <div class="row">
    <div class="col s12">
      <div class="card-panel">
        <h5>Entrada de NF-e</h5>
        <%= form_tag import_nfe_items_path, multipart: true do %>
        <div class="row">
          <div class="input-field col s12">
            Nota XML<br />
            <%= file_field_tag :nfe, class: "round full-width-input" %>
          </div>
        </div>

        <div class="section">
          <%= button_tag type: "submit", name: "action", class: "btn" do %>
            <i class="material-icons right">send</i>Enviar
          <% end %>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
