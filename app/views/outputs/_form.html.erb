<%= form_for @output do |f| %>
  <div class="row">
    <div class="input-field col s12">
      <%= f.collection_select :stock_id, Stock.all, :id, :name %>
      <%= f.label :stock, "Loja" %>
    </div>
  </div>

<div class="row">
    <div class="input-field col s12">
      <%= f.collection_select :user_id, User.all, :id, :name %>
      <%= f.label :user, "ResponsÃ¡vel" %>
    </div>
  </div>

  <div class="row">
    <div class="input-field col s12">
